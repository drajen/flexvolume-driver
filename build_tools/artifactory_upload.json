{
    "files": [
        {
            "pattern": "buildinfo.txt",
            "props": "buildno=${buildno};job_name=${JOB_NAME};jobno=${BUILD_NUMBER}",
            "target": "dcs_k8s-local/${JOB_NAME}/${BUILD_NUMBER}/",
            "recursive": "false"
        },
         {
            "pattern": "src/github.com/hpe-storage/flexvolume-driver/build/flexvolume-driver.exe",
            "props": "buildno=${buildno};job_name=${JOB_NAME};jobno=${BUILD_NUMBER}",
            "target": "dcs_k8s-local/${JOB_NAME}/${BUILD_NUMBER}/bin/"
        }
   ]
}